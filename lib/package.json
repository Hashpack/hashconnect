{
  "name": "hashconnect",
  "version": "0.1.5",
  "description": "hashconnect interoperability library",
  "author": "Tyler Cot√© <tyler@hashpack.app>, Nick Hanna <nick@hashpack.app>",
  "repository": "https://github.com/Hashpack/hashconnect",
  "license": "MIT",
  "keywords": [
    "hashgraph",
    "hedera",
    "wallet",
    "crypto"
  ],
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/"
  ],
  "readme": "../README.md",
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build-watch": "tsc -w -p tsconfig.json",
    "build-all": "yarn clean && yarn build",
    "copy-readme": "cp ../README.md README.md",
    "copy-changelog": "cp ../CHANGELOG.md CHANGELOG.md",
    "prepack": "npm run build && npm run copy-readme && npm run copy-changelog"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^15.0.1",
    "@types/secp256k1": "^4.0.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "esbuild": "^0.13.12",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto-es": "^1.2.7",
    "crypto-js": "^4.1.1",
    "i": "^0.3.7",
    "protons": "^2.0.3",
    "simple-crypto-js": "^3.0.1",
    "ts-typed-events": "3.0.0",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  }
}
